{
  "configuration": {
    "name": "Default",
    "status": "info",
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {"pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"}
      },
      "File": {
        "name": "File",
        "fileName": "loader.log",
        "PatternLayout": {"pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"}
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": [
          {"ref": "STDOUT"},
          {"ref": "File"}
        ]
      }
    }
  }
}